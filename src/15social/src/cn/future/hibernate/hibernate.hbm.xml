<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.future.social.pojo">

	<!-- 社会账户绑定 -->
	<class name="PSocialBind" table="tsocialbind">
		<id name="id" column="id">
			<generator class="assigned" />
		</id>
		<property name="bindType" />
		<property name="bindTypeId" />
		<property name="bindNickName" />
		<property name="accessToken" />
		<property name="userId" />
		<property name="bindPosition" />
		<property name="bindDate" />
	</class>
	
	
	
</hibernate-mapping>